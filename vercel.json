{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ],
    "env": {
        "NODE_ENV": "production",
        "DBHOST": "@dbhost",
        "DBNAME": "@dbname",
        "DBUSER": "@dbuser",
        "DBPORT": "@dbport",
        "JWTKEY": "@jwtkey",
        "FRONTENDURI": "@frontenduri",
        "BACKURI": "@backuri"
    }
} 